<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd"
        logicalFilePath="src/main/resources/liquibase/create-tables-changelog-1.0.xml">

    <changeSet author="anandJ" id="createTelemetryTables-1">
        <createTable catalogName="modeldb"
                     schemaName="public"
                     tableName="modeldb_deployment_info">
            <column name="mdi_key" type="varchar(50)"/>
            <column name="mdi_value" type="varchar(50)"/>
            <column name="creation_timestamp" type="int8"/>
        </createTable>

        <createTable catalogName="modeldb"
                     schemaName="public"
                     tableName="telemetry_information">
            <column name="ti_key" type="varchar(50)"/>
            <column name="ti_value" type="varchar(50)"/>
            <column name="collection_timestamp" type="int8"/>
            <column name="transfer_timestamp" type="int8"/>
            <column name="telemetry_consumer" type="varchar(255)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>